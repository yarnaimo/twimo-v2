{
  "name": "twimo-v2",
  "version": "0.3.1",
  "repository": "github:yarnaimo/twimo-v2",
  "license": "MIT",
  "author": "yamaimo <yarnaimo@gmail.com>",
  "type": "module",
  "exports": "./dist/src/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && node add-import-extension.js openapi",
    "dev": "yarn ts-node src/index.ts",
    "lint": "eslint --ignore-path .gitignore '**/*.{js,jsx,ts,tsx}'",
    "openapi:generate": "./openapi-generate.sh",
    "prepare": "yarn build",
    "release": "yarn openapi:generate && np --no-cleanup",
    "test": "NODE_ENV=test jest",
    "ts-node": "node --loader ts-node/esm --experimental-repl-await",
    "ts-node:debug": "yarn ts-node --inspect"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "oauth-1.0a": "^2.2.6"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.6",
    "@openapitools/openapi-generator-cli": "^2.4.5",
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/jest": "^27.0.2",
    "@types/prettier": "^2.4.0",
    "@yarnaimo/eslint-config": "^0.2.3",
    "enhanced-resolve": "^5.8.3",
    "jest": "^27.2.2",
    "np": "^7.5.0",
    "prettier": "^2.4.1",
    "sort-package-json": "^1.52.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsd": "^0.17.0",
    "type-fest": "^2.3.4",
    "typescript": "^4.4.3"
  }
}
